{
  "name": "note toc",
  "description": "a google chrome extension to add table of contents to articles of note.com",
  "version": "0.9.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://note.com/*"],
      "run_at": "document_end",
      "css": [
        "src/tocbot.css",
        "src/style.css"
      ],
      "js": [
        "src/tocbot.min.js",
        "src/content-script.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}